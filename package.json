{
  "name": "@eliware/module-template",
  "type": "module",
  "version": "1.1.0",
  "description": "A Node.js module-template.",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "index.mjs",
    "index.cjs",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliware/module-template.git"
  },
  "keywords": [
    "nodejs",
    "module",
    "template",
    "boilerplate",
    "javascript",
    "typescript",
    "esm",
    "commonjs",
    "cjs",
    "package",
    "library",
    "npm",
    "testing",
    "jest",
    "ci",
    "continuous-integration",
    "node"
  ],
  "author": "Eli Sterling, eliware.org <eli@eliware.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliware/module-template/issues"
  },
  "homepage": "https://github.com/eliware/module-template#readme",
  "devDependencies": {
    "jest": "^30.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "**/?(*.)+(spec|test).mjs",
      "**/?(*.)+(spec|test).cjs"
    ]
  },
  "dependencies": {
    "@eliware/common": "^1.1.1"
  }
}
